<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.superkind.nhnsoft_wikiviewer">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_wikipedia"
        android:label="${appLabel}"
        android:roundIcon="@mipmap/ic_wikipedia_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NhnSoft_WikiViewer">

        <activity android:name="com.superkind.nhnsoft_wikiviewer.activity.WikiSearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.superkind.nhnsoft_wikiviewer.activity.WebViewActivity"/>

    </application>

</manifest>